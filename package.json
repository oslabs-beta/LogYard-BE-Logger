{
  "name": "logyardbackend",
  "version": "1.0.64",
  "description": "Winston wrapper for backend logging",
  "source": "index.js",
  "main": "dist/indexCJS.cjs",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:commonjs": "babel index.js -o indexCJS.cjs",
    "build:umd": "cross-env FILE_NAME=logyardfrontend webpack",
    "build:umd:min": "cross-env NODE_ENV=production npm run build:umd",
    "clean": "rimraf lib dist",
    "build": "npm run clean && npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "buildParcel": "parcel build"
  },
  "author": "OSP7",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.9",
    "winston": "^3.10.0",
    "winston-mongodb": "^5.1.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "parcel": "^2.9.3",
    "path": "^0.12.7",
    "rimraf": "^5.0.1"
  }
}
