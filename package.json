  "license": "ISC",
  "dependencies": {
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "winston": "^3.10.0",
    "winston-mongodb": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "cross": "^1.0.0",
    "env": "^0.0.2",
    "rimraf": "^5.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
